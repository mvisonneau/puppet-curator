{
  "name": "mvisonneau-curator",
  "version": "1.0.9",
  "author": "Maxime VISONNEAU",
  "summary": "Installs, configures, and manages Elasticsearch Curator.",
  "license": "Apache-2.0",
  "source": "https://github.com/mvisonneau/puppet-curator",
  "project_page": "https://github.com/mvisonneau/puppet-curator",
  "issues_url": "https://github.com/mvisonneau/puppet-curator",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.3.0 < 8.0.0"
    }
  ],
  "dependencies": [
    {
      "name":"puppetlabs/stdlib",
      "version_requirement":">=  8.1.0 < 9.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 8.3.0 < 9.0.0"
    },
    {
      "name": "puppet/cron",
      "version_requirement": ">= 3.0.0 < 4.0.0"
    },
    {
      "name": "puppetlabs/yumrepo_core",
      "version_requirement": ">= 1.1.0 < 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
