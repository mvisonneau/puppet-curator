## MANAGED BY PUPPET ##
client:
  hosts:
<% @hosts.sort.each do |host| -%>
    - <%= host %>
<% end -%>
  port: <%= @port %>
  url_prefix: <%= @url_prefix %>
  use_ssl: <%= @use_ssl.to_s.capitalize %>
  certificate: <%= @certificate %>
  client_cert: <%= @client_cert %>
  client_key: <%= @client_key %>
  ssl_no_validate: <%= @ssl_no_validate.to_s.capitalize %>
<% @ssl_auth.each do |option_line| -%>
  <%= option_line %>
<% end -%>
  timeout: <%= @timeout %>
  master_only: <%= @master_only.to_s.capitalize %>

logging:
  loglevel: <%= @log_level %>
  logfile: <%= @log_file %>
  logformat: <%= @log_format %>
  blacklist: <%= @log_blacklist %>
